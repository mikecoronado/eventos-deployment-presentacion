

    <main class="contenedor contenedor-formularios no-padding">
        <h1><%- nombrePagina %></h1>

        <form class="default-form" action="/cambiar-password" method="POST">
            <div class="campo">
                <label>PAssword Anterior</label>
                <input type="password" name="anterior" placeholder="Password anterior">
            </div>
            <div class="campo">
                <label>Nuevo PAssword</label>
                <input type="password" name="nuevo" placeholder="Nuevo Password">
            </div>
            
            <div class="campo enviar">
                <input type="submit" class="btn btn-rosa" value="Cambiar password">
            </div>
        </form>
    </main>
